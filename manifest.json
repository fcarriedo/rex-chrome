{
  "name": "Rex",
  "version": "0.9.2",
  "description": "A Convore Chrome Extension.",
  "icons": {
    "16": "resources/images/rex128.png",
    "48": "resources/images/rex128.png",
    "128": "resources/images/rex128.png"
  },
  "background_page": "background.html",
